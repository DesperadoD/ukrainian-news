{
  "manifest_version": 2,

  "name": "Pravda Last News",
  "description": "This extension shows last news from web page - pravda.com.ua",
  "version": "1.0",

  "options_page": "options.html",

  "permissions": [
    //"http://localhost/pravda.xml"
    "http://www.pravda.com.ua/rss/view_news/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  }
}